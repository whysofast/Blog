<%- include('partials/header'); -%>
<h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="inputTitle">Title</label>
            <input class="form-control"type="text" name="postTitle" id="inputTitle" autocomplete="off">
        </div>
        <div class="form-group">
            <label for="inputPost">Post</label>
            <textarea class="form-control"  name="postText" id="inputPost" autocomplete="off"></textarea>
        </div>
            <button class="btn btn-primary mb-2" type="submit" name="button">Publish</button>
    </form>     

<%- include('partials/footer'); -%>